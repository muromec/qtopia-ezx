        <status name="lock" rect="26%,0,9%x0" imageon="lock.svg" on="expr:@/UI/KeyLock" active="expr:@/UI/KeyLock"/>
      </exclusive>
      <status name="roaming" rect="38.32%,0,9%x0" imageon="roaming.svg" on="expr:@/Telephony/Status/Roaming"/>
