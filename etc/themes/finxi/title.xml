      </text>
      </rect>
<exclusive>
      <rect rect="75%,0,27%x0">
        <text outline="#000000" color="#FFFFFF" bold="yes" align="hcenter,vcenter" size="5" active="expr:@/UI/HomeScreen/ShowDate">
        expr:@/UI/DisplayTime/BriefDate
      </text>
      </rect>
